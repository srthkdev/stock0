# API Keys Configuration
# Copy this file to .env and fill in your actual API keys

# Finnhub API Key (required for real stock data)
# Get your free API key at https://finnhub.io/dashboard
FINNHUB_API_KEY=your_finnhub_api_key_here

# OpenAI API Key (optional - for enhanced reasoning)
# Get your API key at https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Tavily API Key (optional - for news data)
# Get your API key at https://tavily.com
TAVILY_API_KEY=your_tavily_api_key_here

# Server Configuration
PORT=8000
HOST=0.0.0.0
DEBUG=True

# Note: The application will work without API keys using mock data
# However, for production use, real API keys are recommended 